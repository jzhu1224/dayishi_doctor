<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="app_primary_color" format="color"/> <!-- topbar -->
    <attr name="app_content_bg_color" format="color"/> <!-- content-->
    <attr name="cardCornerRadius" format="dimension"/> <!-- content-->
    <declare-styleable name="CodeItemView">
        <attr name="length" format="integer"/>
        <attr name="width" format="dimension"/>
        <attr name="divideWidth" format="dimension"/>
        <attr name="height" format="dimension"/>
